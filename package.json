{
	"name": "linearly",
	"version": "0.21.0",
	"description": "Matrix and vector library with immutable data types",
	"main": "./lib/cjs/index.js",
	"module": "./lib/esm/index.js",
	"types": "./lib/esm/index.d.ts",
	"sideEffects": false,
	"scripts": {
		"build": "tsc && tsc -p tsconfig.cjs.json",
		"build:doc": "typedoc",
		"lint": "eslint",
		"test": "vitest",
		"prepare": "npm run build",
		"preversion": "npm run test",
		"postversion": "git push && git push --tags && npm publish"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/baku89/linearly.git"
	},
	"keywords": [
		"matrix",
		"vector",
		"linear",
		"algebra",
		"graphics",
		"transform"
	],
	"author": "Baku Hashimoto <mail@baku89.com>",
	"files": [
		"lib"
	],
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/baku89/linearly/issues"
	},
	"homepage": "https://baku89.github.io/linearly",
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^6.7.0",
		"@typescript-eslint/parser": "^6.7.0",
		"eslint": "^8.49.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-simple-import-sort": "^10.0.0",
		"eslint-plugin-unused-imports": "^3.0.0",
		"prettier": "^3.0.3",
		"ts-node": "^10.9.1",
		"typedoc": "^0.25.1",
		"typedoc-plugin-markdown": "^3.16.0",
		"typescript": "^5.2.2",
		"vitest": "^1.2.2"
	}
}
